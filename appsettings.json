  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "[domain]",
    "TenantId": "[tenandId]",
    "ClientId": "[clientId]",
    "CallbackPath": "/.auth/login/aad/callback"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
